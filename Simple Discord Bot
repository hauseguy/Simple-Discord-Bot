const Discord = require('discord.js');
const client = new Discord.Client();

client.on('ready', () => {
  console.log(`Logged in as ${client.user.tag}!`);
});

client.on('message', msg => {
  if (msg.content === 'bb!plumbit') {
    msg.reply('Here is the link to some of the Minecraft parodies. https://www.youtube.com/channel/UCndEIplz0_m72GnuYbA5vVg');
  }
});
client.login('NzQwNzUzNjE1ODQ0NTQwNDYy.XytmkA.tp8scHSyVTMpBx6bfF2MWtGrpxg');
